end {
  my_array = [2, 9, 10, 3, 1, 4, 5, 8, 7, 6];

  print "Original:";
  print my_array;

  print;
  print "Ascending:";
  print sort(my_array);
  print sort(my_array, func (a,b) { return a <=> b });

  print;
  print "Descending:";
  print sort(my_array, "r");
  print sort(my_array, func (a,b) { return b <=> a });
}
