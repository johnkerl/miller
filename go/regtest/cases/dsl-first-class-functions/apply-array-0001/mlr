end {
  my_array = [2, 9, 10, 3, 1, 4, 5, 8, 7, 6];
  print "Original:";
  print my_array;

  print;
  print "Squares:";
  print apply(my_array, func(e) { return e**2 });

  print;
  print "Cubes:";
  print apply(my_array, func(e) { return e**3 });

  print;
  print "Sorted cubes:";
  print sort(apply(my_array, func(e) { return e**3 }));
}
