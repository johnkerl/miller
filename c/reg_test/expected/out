================================================================
LASHED EMITP SINGLES

mlr -n put end {
  @a = 111;
  emitp @a
}
a=111

mlr -n put end {
  @a = 111;
  emitp (@a)
}
a=111

mlr -n put end {
  @a[111] = 222;
  emitp @a, "s"
}
s=111,a=222

mlr -n put end {
  @a[111] = 222;
  emitp (@a), "s"
}
s=111,a=222

mlr -n put end {
  @a[111] = 222;
  @a[333] = 444;
  emitp @a, "s"
}
s=111,a=222
s=333,a=444

mlr -n put end {
  @a[111] = 222;
  @a[333] = 444;
  emitp (@a), "s"
}
s=111,a=222
s=333,a=444

mlr -n put end {
  @a[111][222] = 333;
  emitp @a, "s"
}
s=111,a:222=333

mlr -n put end {
  @a[111][222] = 333;
  emitp (@a), "s"
}
s=111,a:222=333

mlr -n put end {
  @a[111][222] = 333;
  @a[444][555] = 666;
  emitp @a, "s"
}
s=111,a:222=333
s=444,a:555=666

mlr -n put end {
  @a[111][222] = 333;
  @a[444][555] = 666;
  emitp (@a), "s"
}
s=111,a:222=333
s=444,a:555=666

mlr -n put end {
  @a[111][222] = 333;
  emitp @a, "s", "t"
}
s=111,t=222,a=333

mlr -n put end {
  @a[111][222] = 333;
  emitp (@a), "s", "t"
}
s=111,t=222,a=333

mlr -n put end {
  @a[111][222] = 333;
  emitp @a[111], "t"
}
t=222,a=333

mlr -n put end {
  @a[111][222] = 333;
  emitp (@a[111]), "t"
}
t=222,a=333


