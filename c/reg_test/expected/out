================================================================
LASHED EMITP PAIRS

mlr -n put end {
  @a = 111;
  @b = 222;
  emitp (@a, @b)
}
a=111,b=222

mlr -n put end {
  @a[1] = 111;
  @b[1] = 222;
  emitp (@a[1], @b[1])
}
a=111,b=222

mlr -n put end {
  @a[1][2][3] = 4;
  @b[1][2][3] = 8;
  emitp (@a, @b), "s", "t", "u"
}
s=1,t=2,u=3,a=4,b=8

mlr -n put end {
  @a[1][2][3] = 4;
  @b[5][2][3] = 8;
  emitp (@a[1], @b[5]), "t", "u"
}
t=2,u=3,a=4,b=8

mlr -n put end {
  @a[1][2][3] = 4;
  @b[5][6][3] = 8;
  emitp (@a[1][2], @b[5][6]), "u"
}
u=3,a=4,b=8


