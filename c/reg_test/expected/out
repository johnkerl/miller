---------------------------------------------------------------- key-value fors with map values
mlr --from ./reg_test/input/abixy-het put 
  ab = $a . "_" . $b;
  xy = $x . "_" . $y;
  map o = {};
  o[ab] = {"foo": xy};
  for (k, v in o) {
    print "k is " . k . "  v is ";
    dump v;
  }

