================================================================
MAPVAR EMIT

mlr -n put end {
  @a[2][3] = 4;
  b[2][3] = 8;
  emit  (@a, b, {2:{3:12}});
  emitp (@a, b, {2:{3:12}});
  emit {};
  emit  (@a, b, {2:{3:12}}), "t";
  emitp (@a, b, {2:{3:12}}), "t";
  emit {};
  emit  (@a, b, {2:{3:12}}), "t", "u";
  emitp (@a, b, {2:{3:12}}), "t", "u";
}
3=12
a:2:3=4,b:2:3=8,_:2:3=12

t=2,3=12
t=2,a:3=4,b:3=8,_:3=12

t=2,u=3,a=4,b=8,_=12
t=2,u=3,a=4,b=8,_=12

mlr -n put end {
  @a[2][3] = 4;
  b[2][3] = 8;
  emitp (@a, b, {2:{3:12}});
  emit  (@a, b, {2:{3:12}});
  emit {};
  emitp (@a, b, {2:{3:12}}), "t";
  emit  (@a, b, {2:{3:12}}), "t";
  emit {};
  emitp (@a, b, {2:{3:12}}), "t", "u";
  emit  (@a, b, {2:{3:12}}), "t", "u";
}
a:2:3=4,b:2:3=8,_:2:3=12
3=12

t=2,a:3=4,b:3=8,_:3=12
t=2,3=12

t=2,u=3,a=4,b=8,_=12
t=2,u=3,a=4,b=8,_=12


